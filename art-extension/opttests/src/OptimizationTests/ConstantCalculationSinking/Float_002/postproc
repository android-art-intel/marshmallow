packagedir=`echo "$(cd $(dirname $0); pwd)/../"`

declare -a testMethodsCounts=('testLoopAddLess:0' 'testLoopAddGreater:0' 'testLoopAddInfinity:1' 'testLoopSubLess:0' 'testLoopSubGreater:0' 'testLoopSubInfinity:1'  'testLoopMulLess:0' 'testLoopMulGreater:0' 'testLoopMulInfinity:1' 'testLoopMulInfinityNeg:0'  'testLoopMulZero:1' 'testLoopMulZeroNeg:0'  'testLoopDivLess:0' 'testLoopDivGreater:0' 'testLoopDivZero:1' 'testLoopDivZeroNeg:0' 'testLoopDivInfinity:1' 'testLoopDivInfinityNeg:0'  'testLoopRemLess:1' 'testLoopRemGreater:1' 'testLoopRemZero:1' 'testLoopNaN:0' 'testLoopInfinity1:1' 'testLoopInfinity2:0' 'testLoopInfinity3:1'  'testLoopUnkownStart:0')

# include the package's postproc
if [ -f $packagedir/postproc_common ]; then
    source $packagedir/postproc_common
fi

