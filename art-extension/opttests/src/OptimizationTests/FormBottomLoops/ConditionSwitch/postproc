packagedir=`echo "$(cd $(dirname $0); pwd)/../"`

declare -a testMethodsCounts=('test1:Rewrite loop' 'test2:Rewrite loop' 'test3:Rewrite loop' 'test4:Rewrite loop' 'test4a:Rewrite loop' 'test5:More than 5 blocks in loop!Rewrite loop'  'test6:Rewrite loop==2' 'test6a:Rewrite loop==2'  'test7:More than 5 blocks in loop!Rewrite loop' )

# include the package's postproc
if [ -f $packagedir/postproc_common ]; then
    source $packagedir/postproc_common
fi

